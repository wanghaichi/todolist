doctype html(lang='zh-cn')
head
    meta(charset='utf-8')

    meta(http-equiv='X-UA-Compatible', content='IE=edge')

    meta(name='viewport', content='width=device-width, initial-scale=1')

    title title

    link(href='css/bootstrap.css', rel='stylesheet')

    link(href='css/login.css', rel='stylesheet')

body
    .container
        form.form-signin(role='form')
            h2.form-signin-heading Please register

            input.form-control(id='uname', type='text', name='uname', placeholder='Name', required='', autofocus='')
            label(for='uname', id= 'lname', style='display: none') 用户名已存在
            input.form-control(id='upwd', type='password', name='upwd', placeholder='Password', required='')

            button.btn.btn-lg.btn-primary.btn-block(type='button', id='registerSubmit') Sign up
    // jQuery (necessary for Bootstrap's JavaScript plugins)
    script(src='js/jquery-1.11.0.min.js')
    // Include all compiled plugins (below), or include individual files as needed
    script(src='js/bootstrap.min.js')
    script(src='js/register.js')
